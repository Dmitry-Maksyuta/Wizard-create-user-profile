{
  "name": "react-typescript-toolkit-saga-enzyme",
  "version": "0.0.1",
  "private": true,
  "description": "Redux Typescript Boilerplate",
  "author": "Maksyuta Dmitry <dimaswagerr@gmail.com>",
  "productName": "Boilerplate",
  "main": "index.tsx",
  "homepage": "./",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/node": "12.12.26",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.2",
    "eslint-plugin-react-hooks": "2.3.0",
    "polished": "^3.6.5",
    "prettier": "2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-use-media": "^0.3.0",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1",
    "typescript": "~3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts",
      "<rootDir>/src/**/*.spec.tsx"
    ],
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.ts"]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/styled-components": "^5.1.2",
    "concurrently": "^5.2.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4"
  }
}
